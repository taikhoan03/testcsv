<style>
    .noti-red{
        background-color:red;
    }
    .noti-green{
        background-color:green;
    }
    .noti-gray{
        background-color:gray;
    }
    .tr_selected{
        background:#2fd2c8;
        
    }
    #data tbody tr>td{
        cursor:pointer;
    }
    #lblLoading {
        color: #ff0000;
        font-size: 14pt;
    }

    .content-wrapper {
        overflow: auto;
    }

    .table {
        white-space: nowrap;
        border-collapse: collapse;
        border-spacing: 0px;
        border: solid 1px rgba(48, 44, 44, 0.90);
        border-radius: 4px;
    }

        .table tr > th, .table tr > td {
            padding: 2px 4px 2px 4px;
            border-right: solid 1px #818181;
        }

    #tSample .table td {
        white-space: pre;
    }

